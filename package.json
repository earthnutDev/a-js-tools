{
  "type": "module",
  "version": "0.1.1",
  "name": "a-js-tools",
  "main": "cjs/index.cjs",
  "module": "mjs/index.mjs",
  "typings": "types/index.d.ts",
  "description": "Some functions that are available in js",
  "files": [
    "mjs",
    "cjs",
    "types",
    "CHANGELOG-en.md",
    "CHANGELOG.md"
  ],
  "exports": {
    "import": "./mjs/index.mjs",
    "require": "./cjs/index.cjs",
    "types": "./types/index.d.ts"
  },
  "scripts": {
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "clean:package": "node scripts/clean-package-json.js",
    "build": "npx ixxx rm dist && npm run b && npm run clean:package",
    "push:version": "npm run beautify && bash -c 'git add . && git commit  -m \"version: $1 $(date +\"%Y-%m-%d %H:%M:%S\")${2:+ }$2\" && git tag -a \"v$1\" -m \"$(date +\"%Y-%m-%d %H:%M:%S\")${2:+ }$2\" && git push origin main --tags' -- ",
    "push:submit": "npm run beautify && bash -c 'git add . && git commit -m \"submit: $(date +\"%Y-%m-%d %H:%M:%S\")${1:+ }$1\" && git push origin main' -- ",
    "clean": "npx ixxx up -d r node_modules package-lock.json && npm install && npm run build",
    "eslint": "npx ixxx cls && eslint src",
    "beautify": "npm run prettier",
    "prettier": "npx ixxx cls && prettier . --write",
    "test": "npx ixxx clear && npx jest --coverage",
    "coverage": "codecov",
    "report": "cat ./coverage/lcov.info | coveralls",
    "versionPatch": "npx ixxx up -n",
    "pub": "npm run build &&  cd dist && npm publish --access public && cd ../",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/earthnutDev/a-js-tools.git"
  },
  "keywords": [
    "a-js-tools"
  ],
  "author": {
    "name": "earthnut",
    "email": "earthnut.dev@outlook.com",
    "url": "https://earthnut.dev/about"
  },
  "homepage": "https://earthnut.dev/a-js-tools",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/earthnutDev/a-js-tools/issues",
    "email": "earthnut.dev@outlook.com"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "a-type-of-js": ">=0.1.5 <1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.23.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/expect": "^24.3.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.11",
    "a-node-tools": "^0.0.15",
    "babel-jest": "^29.7.0",
    "codecov": "^3.8.2",
    "coveralls": "^3.1.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.8",
    "expect": "^29.7.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "rollup": "^4.36.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
