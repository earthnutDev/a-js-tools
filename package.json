{
  "type": "module",
  "version": "0.0.3",
  "name": "a-js-tools",
  "main": "cjs/index.cjs",
  "module": "mjs/index.mjs",
  "typings": "types/index.d.ts",
  "description": "Some functions that are available in js",
  "files": [
    "cjs/",
    "mjs/",
    "types/"
  ],
  "exports": {
    "import": "./mjs/index.mjs",
    "require": "./cjs/index.cjs",
    "types": "./types/index.d.ts"
  },
  "scripts": {
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "build": "npx ixxx rm dist && npm run b",
    "push:deploy": "bash -c 'git add . && git commit -m \"version: ${1+ }$1 $(date +\"%Y-%m-%d %H:%M:%S\")${2:+ }$2\" && git push origin main' -- ",
    "push:submit": "bash -c 'git add . && git commit -m \"submit: $(date +\"%Y-%m-%d %H:%M:%S\")${1:+ }$1\" && git push origin main' -- ",
    "clean": "npx ixxx up -d r node_modules package-lock.json && npm install && npm run build",
    "prettier": "npx prettier . --write",
    "test": "npx ixxx clear && npx jest --coverage",
    "coverage": "codecov",
    "report": "cat ./coverage/lcov.info | coveralls",
    "versionPatch": "npm version patch --force  --no-git-tag-version  --allow-same-version=true",
    "pub": "npm run build &&  cd dist && npm publish  && cd ../"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/earthnutDev/a-js-tools.git"
  },
  "keywords": [
    "a-js-tools"
  ],
  "author": {
    "name": "earthnut",
    "email": "earthnut.dev@outlook.com",
    "url": "https://earthnut.dev/about"
  },
  "homepage": "https://earthnut.dev/a-js-tools",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/earthnutDev/a-js-tools/issues",
    "email": "earthnut.dev@outlook.com"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "a-type-of-js": "^0.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.6.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/expect": "^1.20.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.9",
    "babel-jest": "^29.7.0",
    "codecov": "^3.8.2",
    "coveralls": "^3.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "expect": "^29.7.0",
    "globals": "^15.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.3.2",
    "rollup": "^4.35.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0"
  }
}